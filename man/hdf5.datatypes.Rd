\name{hdf5.datatypes}
\alias{hdf5.datatypes}
\non_function{}
\title{ Mapping from specified types to HDF5 Types.  }
\description{
  A number of different data formats are supported.
  The list below describes the character string used to represent a type
  and the corresponding HDF5 data type.
 \describe{
   \item{"char"}{H5T_NATIVE_CHAR}
   \item{"uchar"}{H5T_NATIVE_UCHAR}
   \item{"short"}{H5T_NATIVE_SHORT}
   \item{"ushort", H5T_NATIVE_USHORT}
  \item{"int"}{H5T_NATIVE_INT}
  \item{"uint"}{H5T_NATIVE_UINT}
  \item{"long"}{H5T_NATIVE_LONG}
  \item{"ulong"}{H5T_NATIVE_ULONG}
  \item{"float"}{H5T_NATIVE_FLOAT}
  \item{"double"}{H5T_NATIVE_DOUBLE}
}
}
